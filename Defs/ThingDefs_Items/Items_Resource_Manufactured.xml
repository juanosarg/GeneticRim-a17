   <!--=============== Organic Pulp ====================-->
   
<ThingDef ParentName="ResourceBase">
    <defName>OrganicPulp</defName>
    <label>organic pulp</label>
    <description>This organic pulp can be used as an organic growth compound to accelerate some of the genetic processes. Needed to upgrade the Gene Pod.</description>
    <graphicData>
      <texPath>Things/Item/OrganicPulp</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>75</stackLimit>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>2</MarketValue>
      <Mass>0.6</Mass>
      <Flammability>1.0</Flammability>
	  <DeteriorationRate>10.0</DeteriorationRate>
	</statBases>
    
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
</ThingDef>

