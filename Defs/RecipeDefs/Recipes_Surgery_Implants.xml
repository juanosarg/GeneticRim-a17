  <RecipeDef ParentName="SurgeryFlesh">
		<defName>InstallBearClaws</defName>
		<label>install bear claws</label>
		<description>Installs bear claws.</description>
		<workerClass>Recipe_InstallImplant</workerClass>
		<jobString>Installing bear claws.</jobString>
		<workAmount>2000</workAmount>
		<recipeUsers>
         <li>Human</li>
        </recipeUsers>
		<ingredients>
			<li>
				<filter>
          <categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BearClaws</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
				<li>BearClaws</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>LeftHand</li> 
			<li>RightHand</li> 
		</appliedOnFixedBodyParts>
		<addsHediff>BearClaws</addsHediff>
	</RecipeDef>
	
