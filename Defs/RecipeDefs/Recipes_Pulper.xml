<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	
	
	<RecipeDef>
		<defName>PulpWood</defName>
		<label>extract organic pulp from wood</label>
		<description>Extract organic pulp from wood.</description>
		<jobString>Extracting Pulp</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
				<li>Pulper</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
		</defaultIngredientFilter>
		<products>
			<OrganicPulp>1</OrganicPulp>
		</products>
	</RecipeDef>
	
	<RecipeDef>
		<defName>PulpCorpse</defName>
		<label>extract organic pulp from corpse</label>
		<description>Extract organic pulp from corpse.</description>
		<jobString>Extracting Pulp</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
				<li>PulpRecycler</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Corpses</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Corpses</li>
			</categories>
			<exceptedCategories>
				<li>CorpsesMechanoid</li>
			</exceptedCategories>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>CorpsesAnimal</li>
			</categories>
		</defaultIngredientFilter>
		<products>
			<OrganicPulp>2</OrganicPulp>
		</products>
	</RecipeDef>
	
	

  
</RecipeDefs>