	<RecipeDef>
		<defName>CreateBearBoomalopeIncubator</defName>
		<label>bear / boomalope incubator</label>
		<description>Fill an incubator with genes from a bear and a boomalope</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>GenePod</li>	
			<li>AdvancedGenePod</li>	  

		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BearGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BoomalopeGenetic</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>EmptyIncubator</li>
				<li>BearGenetic</li>
				<li>BoomalopeGenetic</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<BearBoomalopePre>1</BearBoomalopePre>
		</products>
	</RecipeDef>

