<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

	
	

	
	<RecipeDef>
		<defName>ExtractSerumBear</defName>
		<label>extract alpha serum from a bear</label>
		<description>Extract alpha serum from a bear corpse.</description>
		<jobString>Extracting alpha serum</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<workSkill>Intellectual</workSkill>
		<recipeUsers>
				<li>AlphaSerumExtractionTable</li>	  
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
				
					<thingDefs>
						<li>GrizzlyBear_Corpse</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		
			<thingDefs>
				<li>GrizzlyBear_Corpse</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<BearSerum>1</BearSerum>
		</products>
		<skillRequirements>
			<li>
				<skill>Intellectual</skill>
				<minLevel>5</minLevel>
			</li>
		</skillRequirements>
	</RecipeDef>
	
